{
  "expo": {
    "name": "WiseDealer",
    "slug": "WiseDealer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/Splash-Screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#0E262F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.warrantywise.dealer",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.warrantywise.dealer",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/icon.png"
    },
    "description": "Cross platform e-commerce car retail app for car dealers. (v0.0.0)"
  }
}
